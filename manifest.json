{
  "manifest_version": 2,

  "name": "Wiki Read it",
  "description": "This extension keeps track of what pages you have read online",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
